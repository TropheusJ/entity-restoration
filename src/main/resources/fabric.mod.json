{
  "schemaVersion": 1,
  "id": "entity_restoration",
  "version": "${version}",
  "name": "Entity Restoration",
  "description": "Lets you copy all entities from one world to another, intended for when you accidentally kill @e.",
  "authors": [
    "Tropheus Jay"
  ],
  "contact": {
    "sources": "https://github.com/TropheusJ/entity-restoration"
  },
  "license": "MIT",
  "icon": "assets/entity_restoration/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.tropheusj.entity_restoration.EntityRestoration"
    ]
  },
  "mixins": [
    "entity_restoration.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": ">=${fabric_version}",
    "minecraft": ">=${minecraft_version}",
    "java": ">=${java_version}"
  }
}
